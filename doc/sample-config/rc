# proxy listen address
listen = 127.0.0.1:7777
# parent socks proxy address
socks = 127.0.0.1:1080
# Nubmer of cores to use
core = 2
# ssh to the given server to start socks proxy
sshServer = gfw
# Update blocked domain list (~/.cow/auto-blocked)
updateBlocked = true
# Update directly accessable domain list (~/.cow/auto-direct)
updateDirect = true
# Automatically retry blocked request instead of sending back error page
autoRetry = true
# empty path means stdout, use /dev/null to disable output
logFile = ~/.cow/log
